package com.codingdojo;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class RootController {
	
	@GetMapping()
	public String index() {
		return "index";
	}

	@GetMapping()
	public String show() {
		return "show";
	}
	
	@GetMapping()
	public String edit() {
		return "edit";
	}
}
